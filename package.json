{
  "name": "directus-migrator",
  "version": "1.3.8",
  "main": "./dist/index.cjs",
  "type": "module",
  "license": "MIT",
  "tsup": {
    "entry": "src/index.ts",
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "fs",
      "path",
      "prompts",
      "args-parser",
      "esm"
    ],
    "dts": true
  },
  "scripts": {
    "build": "npx tsup",
    "dev": "npx tsup --watch",
    "start": "npx tsup --watch --dts"
  },
  "homepage": "https://github.com/CodingButter-LLC/directus-migrator",
  "repository": {
    "type": "git",
    "tsup": "tsup src/index.cjs",
    "url": "https://github.com/CodingButter-LLC/directus-migrator.git"
  },
  "dependencies": {
    "args-parser": "^1.3.0",
    "esm": "^3.2.25",
    "prompts": "^2.4.2"
  },
  "bin": {
    "directus-migrator": "./dist/index.cjs"
  }
}
